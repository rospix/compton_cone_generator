<launch>

  <arg name="uav_name" default="$(optenv UAV_NAME uav)"/>

  <group ns="$(arg uav_name)">

    <node name="compton_cone_generator" pkg="nodelet" type="nodelet" args="standalone compton_cone_generator/ComptonConeGenerator" output="screen">

      <rosparam file="$(find compton_cone_generator)/config/compton_cone_generator.yaml" command="load" />

      <param name="uav_name" value="$(arg uav_name)" />

      <!-- subscribed topics -->
      <remap from="~cluster_list_in" to="compton_camera/cluster_list" />
      <remap from="~cones_out" to="compton_camera/cones" />

    </node>

  </group>

</launch>
